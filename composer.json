{
    "name": "vipertecpro/laravelcdn6",
    "description": "Content Delivery Network (CDN) Package for Laravel",
    "license": "MIT",
    "keywords": [
      "laravel",
      "CDN",
      "upload",
      "AWS",
      "S3",
      "AWS S3",
      "amazon",
      "Assets Upload",
      "Content Delivery Network",
      "AWS CloudFront"
    ],
    "authors": [
        {
            "name": "Vipul Walia",
            "email": "vipertecpro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|6.*",
        "illuminate/config": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|6.*",
        "symfony/finder": "2.*|3.*|4.*|5.*",
        "symfony/console": "2.*|3.*|4.*|5.*",
        "aws/aws-sdk-php": "~3.0",
        "ext-json": "*"
    },
    "require-dev": {
       "roave/security-advisories": "dev-master",
       "phpunit/phpunit" : "^8.0",
       "orchestra/testbench": "~3.8.0|^4.0"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php",
            "src/Vipertecpro/laravelcdn6/Exceptions"
        ],
      "psr-4": {
            "Vipertecpro\\laravelcdn6\\": "src/"
        }
    },
  "extra": {
    "laravel": {
      "providers": [
        "Vipertecpro\\laravelcdn6\\CdnServiceProvider"
      ],
      "aliases": {
        "CDN": "Vipertecpro\\laravelcdn6\\Facades\\CdnFacadeAccessor"
      }
    }
  },
    "minimum-stability": "stable"
}
